@article{saroglouValuesReligiosityMetaanalysis2004,
  title = {Values and Religiosity: A Meta-Analysis of Studies Using {{Schwartz}}'s Model},
  author = {Saroglou, V. and Delpierre, V. and Dernelle, R.},
  year = {2004},
  month = sep,
