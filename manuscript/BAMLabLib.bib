@article{henrichEvolutionCulturalEvolution2003a,
  title = {The Evolution of Cultural Evolution},
  author = {Henrich, Joseph and McElreath, Richard},
  year = {2003},
  volume = {12},
  pages = {123--135},
  issn = {1520-6505},
  abstract = {Humans are unique in their range of environments and in the nature and diversity of their behavioral adaptations. While a variety of local genetic adaptations exist within our species, it seems certain that the same basic genetic endowment produces arctic foraging, tropical horticulture, and desert pastoralism, a constellation that represents a greater range of subsistence behavior than the rest of the Primate Order combined. The behavioral adaptations that explain the immense success of our species are cultural in the sense that they are transmitted among individuals by social learning and have accumulated over generations. Understanding how and when such culturally evolved adaptations arise requires understanding of both the evolution of the psychological mechanisms that underlie human social learning and the evolutionary (population) dynamics of cultural systems.},
  journal = {Evolutionary Anthropology: Issues, News, and Reviews},
  number = {3}
}

@article{henrichEvolutionPrestigeFreely2001,
  title = {The Evolution of Prestige: Freely Conferred Deference as a Mechanism for Enhancing the Benefits of Cultural Transmission},
  shorttitle = {The Evolution of Prestige},
  author = {Henrich, Joseph and {Gil-White}, Francisco J},
  year = {2001},
  month = may,
  volume = {22},
  pages = {165--196},
  issn = {10905138},
  doi = {10.1016/S1090-5138(00)00071-4},
  abstract = {This paper advances an ``information goods'' theory that explains prestige processes as an emergent product of psychological adaptations that evolved to improve the quality of information acquired via cultural transmission. Natural selection favored social learners who could evaluate potential models and copy the most successful among them. In order to improve the fidelity and comprehensiveness of such ranked-biased copying, social learners further evolved dispositions to sycophantically ingratiate themselves with their chosen models, so as to gain close proximity to, and prolonged interaction with, these models. Once common, these dispositions created, at the group level, distributions of deference that new entrants may adaptively exploit to decide who to begin copying. This generated a preference for models who seem generally ``popular.'' Building on social exchange theories, we argue that a wider range of phenomena associated with prestige processes can more plausibly be explained by this simple theory than by others, and we test its predictions with data from throughout the social sciences. In addition, we distinguish carefully between dominance (force or force threat) and prestige (freely conferred deference). D 2001 Elsevier Science Inc. All rights reserved.},
  file = {/Users/willgervais/Zotero/storage/YN8KHX8B/Henrich and Gil-White - 2001 - The evolution of prestige freely conferred defere.pdf},
  journal = {Evolution and Human Behavior},
  language = {en},
  number = {3}
}

