@article{hamlin7montholdInfantsSelectively2008,
  title = {Do as {{I}} Do: 7-Month-Old Infants Selectively Reproduce Others' Goals},
  author = {Hamlin, J. Kiley and Hallinan, Elizabeth V. and Woodward, Amanda L.},
