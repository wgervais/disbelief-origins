@article{kapogiannisCognitiveNeuralFoundations2009,
  title = {Cognitive and Neural Foundations of Religious Belief},
  author = {Kapogiannis, Dimitrios and Barbey, Aron K. and Su, Michael and Zamboni, Giovanna and Krueger, Frank and Grafman, Jordan},
  year = {2009},
