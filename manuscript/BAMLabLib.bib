  keywords = {Character,Climate,Communicable Diseases,Cross-Cultural Comparison,Cross-Sectional Studies,Cultural Characteristics,Extraversion (Psychology),Female,Gender Identity,Humans,Internal-External Control,Male,Personality Inventory,Psychometrics,Sexual Behavior,Social Behavior,Statistics as Topic},
