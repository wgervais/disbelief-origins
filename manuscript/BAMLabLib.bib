@article{aydinTurningGodFace2010,
  title = {Turning to {{God}} in the {{Face}} of {{Ostracism}}: {{Effects}} of {{Social Exclusion}} on {{Religiousness}}},
  author = {Aydin, Niluefer and Fischer, Peter and Frey, Dieter},
  year = {2010},
  month = jun,
  volume = {36},
  pages = {742--753},
  issn = {0146-1672},
  doi = {10.1177/0146167210367491},
  abstract = {The present research proposes that individuals who are socially excluded can turn to religion to cope with the experience. Empirical studies conducted to test this hypothesis consistently found that socially excluded persons reported (a) significantly higher levels of religious affiliation (Studies 1, 2, and 4) and (b) stronger intentions to engage in religious behaviors (Study 2) than comparable, nonexcluded individuals. Direct support for the stress-buffering function of religiousness was also found, with a religious prime reducing the aggression-eliciting effects of consequent social rejection (Study 5). These effects were observed in both Christian and Muslim samples, revealing that turning to religion can be a powerful coping response when dealing with social rejection. Theoretical and practical implications of these findings are discussed.},
  journal = {Personality and Social Psychology Bulletin},
  number = {6}
}

