@article{faulknerEvolvedDiseaseavoidanceMechanisms2004,
  title = {Evolved Disease-Avoidance Mechanisms and Contemporary Xenophobic Attitudes},
  author = {Faulkner, J. and Schaller, M. and Park, J. H. and Duncan, L. A.},
