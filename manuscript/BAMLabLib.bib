  keywords = {Aggression,Altruism,Animals,Anthropology,Behavior; Animal,Biological Evolution,Cooperative Behavior,Cultural Evolution,Ethnic Groups,Female,Hominidae,Hostility,Humans,Male,Models; Theoretical,Pan troglodytes,Power (Psychology),Risk-Taking,Social Behavior,Transients and Migrants,War},
